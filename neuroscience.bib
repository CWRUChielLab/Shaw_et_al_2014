	Mesh = {Animals; Computer Simulation; Fourier Analysis; Geniculate Bodies; Long-Term Potentiation; Models, Neurological; Neuronal Plasticity; Neurons; Orientation; Pattern Recognition, Visual; Photic Stimulation; Receptors, N-Methyl-D-Aspartate; Synapses; Visual Cortex; Visual Fields; Visual Pathways},
	Month = {Sep},
	Number = {9},
	Pages = {2554-80},
	Pmid = {19548800},
	Title = {Constraint on the number of synaptic inputs to a visual cortical neuron controls receptive field formation},
	Volume = {21},
