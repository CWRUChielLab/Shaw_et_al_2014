	Journal = {J Comput Neurosci},
	Journal-Full = {Journal of computational neuroscience},
	Mesh = {Adaptation, Physiological; Algorithms; Animals; Computer Simulation; Models, Neurological; Neural Inhibition; Neuronal Plasticity; Neurons; Periodicity; Potassium Channels, Calcium-Activated; Sodium Channels; Synaptic Transmission; Time Factors},
	Month = {Aug},
	Number = {1},
