	Mesh = {Animals; Cells, Cultured; Electric Impedance; Electrophysiology; Fourier Analysis; Hippocampus; Ion Channels; Markov Chains; Models, Neurological; Neurons; Patch-Clamp Techniques; Picrotoxin; Quinoxalines; Rats; Rats, Wistar; Signal Processing, Computer-Assisted; Synapses},
	Month = {Oct},
